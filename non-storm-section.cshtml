@model List<Exemplar.Web.Areas.PropertyInspection.Models.ReportViewModels.SectionViewModels.NonStormDamageItem>

<div class="SectionLabel">Non Storm Related Damage </div>
@if (Model.Count > 0)
{
  <p class="mt-2">
    @for (int i = 0; i < Model.Count; i++)
    {
      @if (i > 0)
      {
        @Model[i].Item
      }
      else
      {
        @Model[i].Item
      }
    }
  </p>

}
else
{
  <text> NA </text>
}
